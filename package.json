{
  "name": "generator-eslinter",
  "version": "1.1.0",
  "description": "Yeoman generator for .eslintrc file with react support",
  "main": "app/index.js",
  "scripts": {
    "lint:templates": "ejslint app/templates**",
    "lint:generator": "eslint app/index.js",
    "lint": "npm run lint:templates && npm run lint:generator",
    "test": "jest app",
    "test:watch": "jest app --watch",
    "test:coverage": "jest app --coverage",
    "build": "webpack",
    "clean": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/generator-eslinter.git"
  },
  "author": {
    "name": "Yuri Krupnik",
    "email": "krupnik.yuri@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/generator-eslinter/issues"
  },
  "homepage": "https://github.com/yurikrupnik/generator-eslinter#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.7.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "files": [
    "app"
  ],
  "keywords": [
    "yeoman-generator",
    "eslit",
    ".eslintrc",
    ".eslintignore",
    "esling-plugin",
    "esling-config"
  ]
}
